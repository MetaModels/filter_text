{
  "name": "metamodels/filter_text",
  "description": "MetaModels text filter",
  "keywords": [
    "contao",
    "metamodels",
    "filter",
    "text"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/filter_text/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/filter_text"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core": "^3.5.5",
    "contao-community-alliance/composer-plugin": "~2.0",
    "metamodels/core": "~2.0@alpha",
    "contao-community-alliance/dc-general": "~2.0@beta"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.7",
    "phpcq/all-tasks": "~1.1"
  },
  "autoload": {
    "psr-0": {
      "MetaModels\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "MetaModels\\Test\\": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/metamodelsfilter_text"
      },
      "transifex": {
        "project": "metamodels",
        "prefix": "filter_text-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  }
}
