    "type":"contao-module",
    "homepage":"http://now.metamodel.me/",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Henry Lamorski",
            "email":"henry.lamorski@mailbox.org",
            "homepage":"https://github.com/HenryLamorski",
            "role":"Developer"
        }
    ],
    "require":{
        "php":">=5.3",
        "contao/core": ">=3.2,<4.0",
        "contao-community-alliance/composer-plugin": "~2.0",
        "metamodels/core":"~2.0"
